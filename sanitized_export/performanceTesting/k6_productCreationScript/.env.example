# Environment Configuration for Performance Testing
# Copy this file to .env and update with your actual values

# API Configuration
API_BASE_URL=https://app.your-domain.com
AUTH_BASE_URL=https://auth.your-domain.com
LOGOUT_BASE_URL=https://auth.your-domain.com

# Authentication Credentials
DEV_USERNAME=your_username@domain.com
DEV_PASSWORD=your_password
QA_USERNAME=your_username@domain.com
QA_PASSWORD=your_password
STAGING_USERNAME=your_username@domain.com
STAGING_PASSWORD=your_password

# Workspace Configuration
WORKSPACE_NAME=your_workspace
WORKSPACE_ID=67
OWNER_ID=191
DOMAIN_ID=52

# Test Data Configuration
KNOWLEDGE_BASE_ID=your-knowledge-base-id
LLM_MODEL_ID=your-llm-model-id
DATA_DOMAIN=sales

# Performance Test Configuration
TEST_ENVIRONMENT=qa
VIRTUAL_USERS=10
ITERATIONS=1
DURATION=30s

# Knowledge Agent Configuration
KNOWLEDGE_AGENT_DESCRIPTION=Performance testing knowledge agent for benchmarking data products

# For static testing (optional - use either this or dynamic login)
TEST_COOKIE=your_static_test_cookie_here